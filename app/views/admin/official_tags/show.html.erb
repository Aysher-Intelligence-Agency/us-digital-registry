<div class="col-sm-12 col-md-12 main">
  <p>
    <strong>Tag:</strong>
    <%= @tag.tag_text %>
  </p>

  <dl class="dl-horizontal">
    <dt>Social media</dt>
    <dd><%= @tag.outlet_count%></dd>
    <dt>Mobile App Count</dt>
    <dd><%= @tag.mobile_app_count%></dd>
    <dt>Gallery Count</dt>
    <dd><%= @tag.gallery_count%></dd>
  </dl>

  <%= link_to 'Edit', edit_admin_official_tag_path(@tag) %> 
  <%= link_to 'Back', admin_official_tags_path %>
</div>
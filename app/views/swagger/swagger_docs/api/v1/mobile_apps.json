{
  "apiVersion": "1.0",
  "swaggerVersion": "1.2",
  "basePath": "https://localhost:3000/",
  "resourcePath": "mobile_apps",
  "apis": [
    {
      "path": "api/v1/mobile_apps/tokeninput.json",
      "operations": [
        {
          "summary": "Returns a list of tokens to help with search interfaces",
          "notes": "This returns tokens representing agencies, tags, and a basic text search token for the purpose of building search dialogs",
          "parameters": [
            {
              "paramType": "query",
              "name": "q",
              "type": "string",
              "description": "String to compare to the various values",
              "required": false
            }
          ],
          "responseMessages": [
            {
              "code": 200,
              "responseModel": null,
              "message": "Success"
            },
            {
              "code": 404,
              "responseModel": null,
              "message": "Not Found"
            },
            {
              "code": 406,
              "responseModel": null,
              "message": "The request you made is not acceptable"
            },
            {
              "code": 500,
              "responseModel": null,
              "message": "Requested Range Not Satisfiable"
            }
          ],
          "method": "get",
          "nickname": "Api::V1::MobileApps#tokeninput"
        }
      ]
    },
    {
      "path": "api/v1/mobile_apps.json",
      "operations": [
        {
          "summary": "Fetches all mobile apps",
          "notes": "This lists all active mobile apps.  It accepts parameters to perform basic search as well as searching by tag and agency.",
          "parameters": [
            {
              "paramType": "query",
              "name": "q",
              "type": "string",
              "description": "String to compare to the name & short description of the mobile apps.",
              "required": false
            },
            {
              "paramType": "query",
              "name": "agencies",
              "type": "ids",
              "description": "Comma seperated list of agency ids",
              "required": false
            },
            {
              "paramType": "query",
              "name": "tags",
              "type": "ids",
              "description": "Comma seperated list of tag ids",
              "required": false
            },
            {
              "paramType": "query",
              "name": "page_size",
              "type": "integer",
              "description": "Number of results per page",
              "required": false
            },
            {
              "paramType": "query",
              "name": "page",
              "type": "integer",
              "description": "Page number",
              "required": false
            }
          ],
          "responseMessages": [
            {
              "code": 200,
              "responseModel": null,
              "message": "Success"
            },
            {
              "code": 404,
              "responseModel": null,
              "message": "Not Found"
            },
            {
              "code": 406,
              "responseModel": null,
              "message": "The request you made is not acceptable"
            },
            {
              "code": 500,
              "responseModel": null,
              "message": "Requested Range Not Satisfiable"
            }
          ],
          "method": "get",
          "nickname": "Api::V1::MobileApps#index"
        }
      ]
    },
    {
      "path": "api/v1/mobile_apps/{id}.json",
      "operations": [
        {
          "summary": "Fetches a single mobile app item",
          "notes": "This returns an mobile app based on an ID.",
          "parameters": [
            {
              "paramType": "path",
              "name": "id",
              "type": "integer",
              "description": "ID of the mobile app",
              "required": true
            }
          ],
          "responseMessages": [
            {
              "code": 200,
              "responseModel": null,
              "message": "Success"
            },
            {
              "code": 404,
              "responseModel": null,
              "message": "Not Found"
            },
            {
              "code": 406,
              "responseModel": null,
              "message": "The request you made is not available"
            },
            {
              "code": 500,
              "responseModel": null,
              "message": "Requested Range Not Satisfiable"
            }
          ],
          "method": "get",
          "nickname": "Api::V1::MobileApps#show"
        }
      ]
    }
  ]
}
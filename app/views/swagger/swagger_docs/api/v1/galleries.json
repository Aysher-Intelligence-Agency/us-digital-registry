            {
              "paramType": "query",
              "name": "q",
              "type": "string",
              "description": "String to compare to the name of the galleries.",
              "required": false
            },
            {
              "paramType": "query",
              "name": "tags",
              "type": "ids",
              "description": "Comma seperated list of tag ids",
              "required": false
            },
            {
              "paramType": "query",
              "name": "page_size",
              "type": "integer",
              "description": "Number of results per page",
              "required": false
            },
            {
              "paramType": "query",
              "name": "page",
              "type": "integer",
              "description": "Page number",
              "required": false
            }
          ],
          "responseMessages": [
            {
              "code": 200,
              "responseModel": null,
              "message": "Success"
            },
            {
              "code": 404,
              "responseModel": null,
              "message": "Not Found"
            },
            {
              "code": 406,
              "responseModel": null,
              "message": "The request you made is not acceptable"
            },
            {
              "code": 500,
              "responseModel": null,
              "message": "Requested Range Not Satisfiable"
            }
          ],
          "method": "get",
          "nickname": "Api::V1::Galleries#index"
        }
      ]
    },
    {
      "path": "api/v1/galleries/{id}.json",
      "operations": [
        {
          "summary": "Fetches a single gallery item",
          "notes": "This returns an gallery based on an ID.",
          "parameters": [
            {
              "paramType": "path",
              "name": "id",
              "type": "integer",
              "description": "ID of the gallery",
              "required": true
            }
          ],
          "responseMessages": [
            {
              "code": 200,
              "responseModel": null,
